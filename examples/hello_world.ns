myvar = 'hello world'
johnny = 4000

def foo(johnny=3)
 34 + johnny * 3
end

def bar(johnny=3)
 (34 + johnny) * 3
end

print('Negative johnny is', -johnny)

print(foo())
print(foo(242))

if bar() == 111
  print(bar())
end

if bar(242) == 728
  print('wrong!')
else
  print('cool!')
end

print('Johnny is still', johnny)

print(myvar)
